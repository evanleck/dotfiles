PubkeyAcceptedKeyTypes=+ssh-dss

Host *
  AddKeysToAgent yes
  Compression yes
  ControlMaster auto
  ControlPath /tmp/ssh-%r@%h:%p
  IdentityFile ~/.ssh/evanleck
  Protocol 2
  ServerAliveInterval 90
  UseKeychain yes


Host *.amazonaws.com
  IdentityFile ~/.ssh/engage


Host bitbucket.org
  AddKeysToAgent yes
  IdentityFile ~/.ssh/engage
  UseKeychain yes
